{
  "name": "alfred-nutile-inc/env-deployer",
  "description": "Makes deploying Env super easy",
  "authors": [
    {
      "name": "Alfred Nutile Inc",
      "email": "me@alfrednutile.info"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/support": "*",
    "vlucas/phpdotenv": "~1.0",
    "symfony/process": "~2.1",
    "illuminate/contracts": "*",
    "illuminate/console": "*",
    "illuminate/filesystem": "*"
  },
  "require-dev": {
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0",
    "fzaninotto/faker": "1.4.0",
    "symfony/var-dumper": "^2.7"
  },
  "autoload": {
    "psr-4": {
      "AlfredNutileInc\\EnvDeployer\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php",
      "tests/Config.php"
    ]
  }
}
